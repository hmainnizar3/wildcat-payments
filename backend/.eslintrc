{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": "error",
    "no-unused-imports": "error",
    "@typescript-eslint/no-unused-vars": "error",
    "custom/no-unused-message-response": "error"
  }
}
